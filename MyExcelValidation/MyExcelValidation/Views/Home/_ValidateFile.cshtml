@model MyExcelValidation.Models.TemplateFileDTO

@{
    Layout = null;
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <!-- Default input name -->
    <label for="defaultFormNameModalEx">Sheet Name</label>
    @Html.EditorFor(model => model.SheetName, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
    <br>
    <!-- Default input email -->
    <label for="defaultFormEmailModalEx">Select Excel File</label>
    @Html.TextBoxFor(model => model.Sheet, new { type = "file", name = "Sheet", @class = "form-control form-control-sm" })
    <br>

    <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button class="btn btn-info" formenctype="multipart/form-data">Upload</button>
    </div>
}